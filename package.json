{
  "name": "gearguard",
  "version": "1.0.0",
  "description": "GearGuard - Ultimate Maintenance Tracker",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "seed": "node server/seed.js",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.22.1",
    "firebase": "^12.7.0",
    "mongoose": "^7.8.8",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "maintenance",
    "asset-management",
    "tracker"
  ],
  "author": "",
  "license": "MIT"
}
